#include <iostream>

// Function to find the maximum temperature in a 2D sensor grid
int findMaxTemperature(int arr[][5], int rows) {
    int maxTemp = *(*(arr + 0) + 0); // initialize with first element

    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < 5; j++) {
            if (*(*(arr + i) + j) > maxTemp) {
                maxTemp = *(*(arr + i) + j);
            }
        }
    }

    return maxTemp;
}

int main() {
    int sensors[3][5] = {
        {25, 28, 27, 26, 30},
        {24, 31, 29, 27, 28},
        {23, 25, 28, 26, 29}
    };

    int maxTemperature = findMaxTemperature(sensors, 3);

    std::cout << "Maximum temperature: " << maxTemperature << std::endl;

    return 0;
}